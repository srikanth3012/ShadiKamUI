{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/page.jsx",
    "app/event-organizers/page.jsx",
    "app/event-organizers/[id]/page.jsx",
    "app/U/booking/page.jsx",
    "components/User/booking/step4.jsx",
    "app/layout.jsx",
    "app/U/bookings/page.jsx",
    "app/v/dashboard/page.jsx",
    "app/o/events/page.jsx",
    "app/o/booking/page.jsx",
    "app/v/bookings/page.jsx",
    "app/o/event-organizers/loading.jsx",
    "app/v/profile/page.jsx",
    "app/v/[id]/preview/page.jsx",
    "app/v/bookings/loading.jsx",
    "app/o/events/loading.jsx"
  ],
  "exclude": ["node_modules"]
}
